  },
  "dependencies": {
    "@opam/dune": "*",
    "@esy-ocaml/reason": "^3.4.0",
    "refmterr": "*",
    "@opam/ppx_deriving": "*",
    "ocaml": ">=4.7.0 <4.9.0"
  },
  "devDependencies": {
    "ocaml": "~4.7.0"
  }
